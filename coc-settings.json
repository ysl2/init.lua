{
  "Lua.telemetry.enable": false,
  "languageserver": {
    "sourcery": {
      "command": "sourcery",
      "args": ["lsp"],
      "filetypes": ["python"],
      "initializationOptions": {
        "extension_version": "coc.vim",
        "editor_version": "vim"
      },
      "settings": {
        "sourcery": {
          "metricsEnabled": true
        }
      }
    }
  },
  "suggest.completionItemKindLabels": {
    "text": "\ueb8d Text",
    "method": "\uea8c Method",
    "function": "\udb80\ude95 Function",
    "constructor": "\uf976 Constructor",
    "field": "\ueb5f Field",
    "variable": "\uea88 Variable",
    "class": "\ueb5b Class",
    "interface": "\ueb61 Interface",
    "module": "\ueaec Module",
    "property": "\ueb65 Property",
    "unit": "\udb81\udc6d Unit",
    "value": "\uf4f7 Value",
    "enum": "\uea95 Enum",
    "keyword": "\ueb62 Keyword",
    "snippet": "\udb82\udfc3 Snippet",
    "color": "\ueb5c Color",
    "file": "\ueb60 File",
    "reference": "\ueb36 Reference",
    "folder": "\udb80\ude4b Folder",
    "enumMember": "\ueb5e EnumMember",
    "constant": "\ueb5d Constant",
    "struct": "\uea91 Struct",
    "event": "\uea86 Event",
    "operator": "\ueb64 Operator",
    "typeParameter": "\uea92 TypeParameter",
    "default": "\uf29c Default"
  },
  "Lua.diagnostics.globals": ["vim"],
  "diagnostic.errorSign": "\uea87",
  "diagnostic.warningSign": "\uea6c",
  "diagnostic.infoSign": "\uea74",
  "diagnostic.hintSign": "\uea6b",
  "sumneko-lua.enableNvimLuaDev": true,
  "rust-analyzer.cargo.buildScripts.enable": false,
  "rust-analyzer.procMacro.enable": false,
  "suggest.snippetIndicator": "...",
  "tabnine.enablePreselect": false,
  "python.formatting.provider": "none",
  "python.linting.enabled": false,
  "clangd.arguments": ["-fallback-style=microsoft"],
  "diagnostic-languageserver.mergeConfig": true,
  "diagnostic-languageserver.filetypes": {
    "python": "flake8"
  },
  "diagnostic-languageserver.linters": {
     "flake8": {
       "debounce": 200,
       "rootPatterns": [".git", "pyproject.toml", "setup.py", "requirements.txt", ".root", ".flake8"],
       "securities": {
         "W": "warning",
         "E": "error",
         "F": "info",
         "C": "info",
         "N": "hint"
       }
     }
   },
  "diagnostic-languageserver.formatFiletypes": {
    "python": "black"
  }
}
