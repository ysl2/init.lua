{
  "Lua.telemetry.enable": false,
  "languageserver": {
    "sourcery": {
      "command": "sourcery",
      "args": ["lsp"],
      "filetypes": ["python"],
      "initializationOptions": {
        "extension_version": "coc.vim",
        "editor_version": "vim"
      },
      "settings": {
        "sourcery": {
          "metricsEnabled": true
        }
      }
    }
  },
  "suggest.completionItemKindLabels": {
    "text": "\ueb8d Text",
    "method": "\uea8c Method",
    "function": "\udb80\ude95 Function",
    "constructor": "\uf976 Constructor",
    "field": "\ueb5f Field",
    "variable": "\uea88 Variable",
    "class": "\ueb5b Class",
    "interface": "\ueb61 Interface",
    "module": "\ueaec Module",
    "property": "\ueb65 Property",
    "unit": "\udb81\udc6d Unit",
    "value": "\uf4f7 Value",
    "enum": "\uea95 Enum",
    "keyword": "\ueb62 Keyword",
    "snippet": "\udb82\udfc3 Snippet",
    "color": "\ueb5c Color",
    "file": "\ueb60 File",
    "reference": "\ueb36 Reference",
    "folder": "\udb80\ude4b Folder",
    "enumMember": "\ueb5e EnumMember",
    "constant": "\ueb5d Constant",
    "struct": "\uea91 Struct",
    "event": "\uea86 Event",
    "operator": "\ueb64 Operator",
    "typeParameter": "\uea92 TypeParameter",
    "default": "\uf29c Default"
  },
  "Lua.diagnostics.globals": ["vim"],
  "diagnostic.errorSign": "\uea87",
  "diagnostic.warningSign": "\uea6c",
  "diagnostic.infoSign": "\uea74",
  "diagnostic.hintSign": "\uea6b",
  "sumneko-lua.enableNvimLuaDev": true,
  "rust-analyzer.cargo.buildScripts.enable": false,
  "rust-analyzer.procMacro.enable": false,
  "suggest.snippetIndicator": "...",
  "tabnine.enablePreselect": false,
  // "python.formatting.provider": "black",
  // "python.formatting.blackArgs": [
  //   "--line-length=120",
  //   "--skip-string-normalization"
  // ],
  // "python.linting.flake8Enabled": true,
  // // pip install flake8-annotations. See: https://pypi.org/project/flake8-annotations/
  // "python.linting.flake8Args": [
  //   "--max-line-length=120",
  //   "--ignore=ANN101,ANN102,E402,E741,E203"
  // ],
  "clangd.arguments": ["-fallback-style=microsoft"],
  "markdownlint.config": {
    "default": true,
    "line_length": false
  },
  "diagnostic-languageserver.mergeConfig": true,
  "diagnostic-languageserver.filetypes": {
    "python": "flake8",
  },
  "diagnostic-languageserver.linters": {
     "flake8": {
       "debounce": 200,
       "rootPatterns": [".git", "pyproject.toml", "setup.py", "requirements.txt", ".root"],
       "args": [
         "--max-line-length=120",
         "--ignore=ANN101,ANN102,E402,E741,E203",
         "--format=%(row)d,%(col)d,%(code).1s,%(code)s: %(text)s",
         "-"
       ],
       "securities": {
         "W": "info",
         "E": "warning",
         "F": "info",
         "C": "info",
         "N": "hint"
       }
     }
   },
  "diagnostic-languageserver.formatFiletypes": {
    "python": "black"
  },
  "diagnostic-languageserver.formatters": {
    "black": {
      "command": "black",
      "args": [
        "--line-length=120",
        "--skip-string-normalization",
        "-q", "-"
      ]
    },
  }
}
